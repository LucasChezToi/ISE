<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE netconf SYSTEM "netconf.dtd">

<!-- Exemple servant à tester quelques fonctions -->

<!--
	Cas à tester
		// Cas premiere node de i et j
